{
  "name": "invoice-api",
  "version": "0.0.1",
  "description": "My Invoice API  npm run seed:run",
  "license": "ISC",
  "private": true,
  "scripts": {
    "typeorm": "env-cmd -f .env tsx ./node_modules/typeorm/cli.js",
    "typeorm:prod": "tsx ./node_modules/typeorm/cli.js",
    "build": "nest build",
    "api": "nest start",
    "api:dev": "nest start --watch",
    "api:debug": "nest start --debug --watch",
    "api:prod": "node build/main",
    "seed:refresh": "node build/database/seeds/seeder --refresh",
    "migration:generate": "npm run typeorm -- migration:generate --dataSource=src/database/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create src/database/migrations/migration",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:run:prod": "npm run typeorm:prod -- --dataSource=build/database/data-source.js migration:run",
    "migration:fakerun": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run --fake",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:run": "env-cmd -f .env tsx src/database/seeds/run-seeds.ts",
    "seed:run:prod": "node build/database/seeds/run-seeds.js",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:integration:watch": "jest --config ./test/jest-integration.json --watch",
    "test:integration:cov": "jest --config ./test/jest-integration.json --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@keyv/redis": "^5.1.4",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/typeorm": "^11.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/xml2js": "^0.4.14",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.5",
    "cacheable": "^2.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^4.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.3",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.15.3",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^24.34.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.27",
    "typeorm-extension": "^3.7.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.1.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.9",
    "@types/bcrypt": "^6.0.0",
    "@types/cache-manager": "^5.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.19.1",
    "@types/supertest": "^6.0.2",
    "env-cmd": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
